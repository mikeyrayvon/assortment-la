{
  "Talent" : {
    "uid" : {
      "type" : "UID",
      "config" : {
        "label" : "UID",
        "placeholder" : "unique-identifier-for-talent-url"
      }
    },
    "name" : {
      "type" : "Text",
      "config" : {
        "label" : "Name",
        "placeholder" : "Talent Name",
        "useAsTitle" : true
      }
    },
    "city" : {
      "type" : "Text",
      "config" : {
        "label" : "City"
      }
    },
    "bio" : {
      "type" : "StructuredText",
      "config" : {
        "multi" : "paragraph, strong, em, hyperlink, list-item, o-list-item",
        "allowTargetBlank" : true,
        "label" : "Bio"
      }
    },
    "pdf" : {
      "type" : "Link",
      "config" : {
        "select" : "media",
        "label" : "Portfolio PDF"
      }
    },
    "website" : {
      "type" : "Link",
      "config" : {
        "select" : "web",
        "label" : "Personal Website",
        "allowTargetBlank" : true
      }
    },
    "instagram" : {
      "type" : "Text",
      "config" : {
        "label" : "Instagram Handle"
      }
    },
    "main_image" : {
      "type" : "Image",
      "config" : {
        "constraint" : {
          "width" : null,
          "height" : null
        },
        "thumbnails": [
          {
            "name": "mobile",
            "width": 700,
            "height": null
          },
          {
            "name": "md",
            "width": 900,
            "height": null
          },
          {
            "name": "xl",
            "width": 1100,
            "height": null
          },
          {
            "name": "full",
            "width": 1300,
            "height": null
          }
        ],
        "label" : "Main Image"
      }
    },
    "services" : {
      "type" : "Group",
      "config" : {
        "fields" : {
          "service" : {
            "type" : "Link",
            "config" : {
              "select" : "document",
              "customtypes" : [ "service" ],
              "label" : "Service"
            }
          }
        },
        "label" : "Services"
      }
    },
    "portfolio" : {
      "type" : "Group",
      "config" : {
        "fields" : {
          "image" : {
            "type" : "Image",
            "config" : {
              "constraint" : {
                "width" : null
              },
              "thumbnails": [
                {
                  "name": "mobile",
                  "width": 700,
                  "height": null
                },
                {
                  "name": "sm",
                  "width": 900,
                  "height": null
                },
                {
                  "name": "md",
                  "width": 1200,
                  "height": null
                },
                {
                  "name": "xl",
                  "width": 1500,
                  "height": null
                },
                {
                  "name": "full",
                  "width": 1800,
                  "height": null
                }
              ],
              "label" : "Image"
            }
          },
          "caption" : {
            "type" : "Text",
            "config" : {
              "label" : "Caption"
            }
          },
          "video" : {
            "type" : "Text",
            "config" : {
              "label" : "Video URL"
            }
          }
        },
        "label" : "Portfolio"
      }
    },
    "projects" : {
      "type" : "Group",
      "config" : {
        "fields" : {
          "project" : {
            "type" : "Link",
            "config" : {
              "select" : "document",
              "label" : "Project"
            }
          }
        },
        "label" : "Projects"
      }
    }
  }
}
