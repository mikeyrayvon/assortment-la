{
  "Talent" : {
    "uid" : {
      "type" : "UID",
      "config" : {
        "label" : "UID",
        "placeholder" : "unique-identifier-for-talent-url"
      }
    },
    "name" : {
      "type" : "Text",
      "config" : {
        "label" : "Name",
        "placeholder" : "Talent Name",
        "useAsTitle" : true
      }
    },
    "city" : {
      "type" : "Text",
      "config" : {
        "label" : "City"
      }
    },
    "bio" : {
      "type" : "StructuredText",
      "config" : {
        "multi" : "paragraph, strong, em, hyperlink, list-item, o-list-item",
        "allowTargetBlank" : true,
        "label" : "Bio"
      }
    },
    "pdf" : {
      "type" : "Link",
      "config" : {
        "select" : "media",
        "label" : "Portfolio PDF"
      }
    },
    "website" : {
      "type" : "Link",
      "config" : {
        "select" : "web",
        "label" : "Personal Website",
        "allowTargetBlank" : true
      }
    },
    "instagram" : {
      "type" : "Text",
      "config" : {
        "label" : "Instagram Handle"
      }
    },
    "main_image" : {
      "type" : "Image",
      "config" : {
        "constraint" : {
          "width" : 1200
        },
        "thumbnails" : [ ],
        "label" : "Main Image"
      }
    },
    "services" : {
      "type" : "Group",
      "config" : {
        "fields" : {
          "service" : {
            "type" : "Link",
            "config" : {
              "select" : "document",
              "customtypes" : [ "service" ],
              "label" : "Service"
            }
          }
        },
        "label" : "Services"
      }
    },
    "projects" : {
      "type" : "Group",
      "config" : {
        "fields" : {
          "project" : {
            "type" : "Link",
            "config" : {
              "select" : "document",
              "label" : "Project"
            }
          }
        },
        "label" : "Projects"
      }
    }
  }
}
